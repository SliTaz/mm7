		<State>
			<isFirst>1</isFirst>
			<value>1</value>
			<nextState>2</nextState>
			<refusedState>5</refusedState>
			<name>待内容审核</name>
		</State>

		<State>
			<isFirst>0</isFirst>
			<value>2</value>
			<nextState>3</nextState>
			<refusedState>6</refusedState>
			<name>待测试</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>3</value>
			<nextState>4</nextState>
			<refusedState>7</refusedState>
			<name>待发送审核</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>4</value>
			<nextState>10</nextState>
			<refusedState>11</refusedState>
			<name>待发送</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>5</value>
			<nextState>1</nextState>
			<refusedState>0</refusedState>
			<name>待合成</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>6</value>
			<nextState>2</nextState>
			<refusedState>0</refusedState>
			<name>待合成</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>7</value>
			<nextState>3</nextState>
			<refusedState>0</refusedState>
			<name>待合成</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>8</value>
			<nextState>0</nextState>
			<refusedState>0</refusedState>
			<name>待合成</name>
		</State>
		
		<State>
		<isFirst>0</isFirst>
			<value>9</value>
			<nextState>0</nextState>
			<refusedState>0</refusedState>
			<name>待合成</name>
		</State>


		<State>
		<isFirst>0</isFirst>
			<value>10</value>
			<nextState>0</nextState>
			<refusedState>0</refusedState>
			<name>已发送</name>
		</State>

		<State>
		<isFirst>0</isFirst>
			<value>11</value>
			<nextState>4</nextState>
			<refusedState>0</refusedState>
			<name>暂停</name>
		</State>
	</ContentInfoStates>

